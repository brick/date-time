{
    "name": "brick/date-time",
    "description": "Date and time library",
    "type": "library",
    "keywords": [
        "Brick",
        "DateTime",
        "Date",
        "Time",
        "DayOfWeek",
        "Duration",
        "Instant",
        "Interval",
        "LocalDate",
        "LocalDateRange",
        "LocalDateTime",
        "LocalTime",
        "Month",
        "MonthDay",
        "Period",
        "Stopwatch",
        "TimeZone",
        "Year",
        "YearMonth",
        "YearMonthRange",
        "YearWeek",
        "ZonedDateTime"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "suggest": {
        "ext-timezonedb": "This PECL extension provides up-to-date timezone information"
    },
    "autoload": {
        "psr-4": {
            "Brick\\DateTime\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brick\\DateTime\\Tests\\": "tests/"
        }
    }
}
